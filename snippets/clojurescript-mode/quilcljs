# -*- mode: snippet -*-
# name: quilcljs
# key: quilcljs
# --
(ns $0
  (:require [quil.core :as q :include-macros true]
            [quil.middleware :as md]))

(defn setup [])

(defn update-state [])

(defn draw [])

(defn run [host]
  (q/defsketch $1
    :host host
    :setup setup
    :draw draw
    :update update-state
    :middleware [md/fun-mode]
    :size [300 300]))